import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        System.out.println("Введіть шестизначне число: ");
        int number = scanner.nextInt();

        if (number < 100000 || number > 999999) {
            System.out.println("Ви ввели не шестизначне число");
            return;
        }

        int first = number / 100000;
        int second = (number / 10000) % 10;
        int third = (number / 1000) % 10;
        int fourth = (number / 100) % 10;
        int fifth = (number / 10) % 10;
        int sixth = number % 10;

        int newNumber = (sixth * 100000) + (fifth * 10000) + (third * 1000) + (fourth * 100) + (second * 10) + first;

