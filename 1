import java.util.Scanner;

public class Main
{
    public static void main(String[] args)
    {
        Scanner scanner = new Scanner(System.in);

        System.out.println("Введіть кількість метрів: ");
        double meters = scanner.nextDouble();

        System.out.println("Оберіть, в що перевести метри:");
        System.out.println("1 - Мілі");
        System.out.println("2 - Дюйми");
        System.out.println("3 - Ярди");

        int choice = scanner.nextInt();

        switch (choice)
        {
            case 1:
                double miles = meters / 1609.34;
                System.out.println(meters + " метри = " + miles + " миль.");
                break;
            case 2:
                double inches = meters * 39.3701;
                System.out.println(meters + " метри = " + inches + " дюймів.");
                break;
            case 3:
                double yards = meters * 1.09361;
                System.out.println(meters + " метри = " + yards + " ярдів.");
                break;
            default:
                System.out.println("Невірний вибір. Спробуйте ще раз.");
        }

        scanner.close();
